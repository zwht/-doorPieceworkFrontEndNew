{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [
      "node"
    ],
    "paths": {
      "@interface": [
        "app/interface"
      ],
      "@interface/*": [
        "app/interface/*"
      ],
      "@restFullServer": [
        "app/shared/restFullServer"
      ],
      "@restFullServer/*": [
        "app/shared/restFullServer/*"
      ],
      "@class": [
        "app/class"
      ],
      "@class/*": [
        "app/class/*"
      ],
      "@vo": [
        "app/vo"
      ],
      "@vo/*": [
        "app/vo/*"
      ],
      "@shared": [
        "app/shared/index"
      ],
      "@shared/*": [
        
        "app/shared/*"
      ],
      "@core": [
        "app/core/index"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@env/*": [
        "environments/*"
      ]
    },
    "baseUrl": "./"
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}